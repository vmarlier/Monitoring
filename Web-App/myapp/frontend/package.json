{
  "name": "web-app-frontend",
  "version": "1.0.0",
  "description": "Web application front end",
  "author": "Naomi Paulmin <naomi.paulmin@ynov.com>",
  "private": true,
  "scripts": {
    "build": "node build/build.js",
    "build:report": "npm_config_report=true node build/build.js",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "eslint:check": "eslint --ext .js,.vue src",
    "eslint:fix": "eslint --fix --ext .js,.vue src",
    "eslint:config-check": "eslint --print-config . | eslint-config-prettier-check",
    "lint:check": "npm run prettier:check && npm run eslint:check",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix",
    "prettier:fix": "prettier --write \"src/!(assets)/**/*.{js,vue}\"",
    "prettier:check": "prettier --check \"src/!(assets)/**/*.{js,vue}\"",
    "setup": "npm install && npm run build",
    "start": "npm run dev",
    "test": "npm run lint"
  },
